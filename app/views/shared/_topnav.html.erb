
<% if logged_in? %>
  <p>
    <td><%= link_to 'Test Guru', tests_path %></td><br>
    <td>Welcome, <%= current_user.name %> Guru</td><br>
    <td><%= link_to 'Logout', logout_path, method: :delete %></td>
  </p>
<% else %>
  <p>
    <td><%= link_to 'Log In', login_path %></td><br>
    <td><%= link_to 'Sign Up', signup_path %></td>
  </p>
<% end %>