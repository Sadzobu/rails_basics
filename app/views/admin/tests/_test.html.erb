<tr>
  <td style="text-align: center;"><%= test.title %></td>
  <td style="text-align: center;"><%= test.level %></td>
  <td style="text-align: center;"><%= test.questions.count %></td>
  <td><%= link_to t('actions.show'), admin_test_path(test)%></td>
  <td><%= link_to t('actions.edit'), edit_admin_test_path(test)%></td>
  <td><%= link_to t('actions.delete'), admin_test_path(test),
                            method: :delete,
                            data: { confirm: 'Are you sure?' } %></td>
</tr>
